PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@
INPUT                  = @PROJECT_SOURCE_DIR@/src @PROJECT_SOURCE_DIR@/README.md @PROJECT_SOURCE_DIR@/TODO.md @PROJECT_SOURCE_DIR@/doc
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = README.md
GENERATE_LATEX         = @DOXYGEN_GENERATE_LATEX@
GENERATE_HTML          = @DOXYGEN_GENERATE_HTML@
HTML_OUTPUT            = @DOXYGEN_HTML_DIR@
GENERATE_XML           = @DOXYGEN_GENERATE_XML@
XML_PROGRAMLISTING     = NO
SOURCE_BROWSER         = NO
SHOW_FILES             = NO
VERBATIM_HEADERS       = NO
REFERENCES_LINK_SOURCE = NO
FULL_PATH_NAMES        = NO
STRIP_FROM_PATH        = @CMAKE_SOURCE_DIR@

IMAGE_PATH             = @PROJECT_SOURCE_DIR@/doc/images

EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES

CLASS_DIAGRAMS         = YES
HIDE_UNDOC_RELATIONS   = NO
HAVE_DOT               = YES
CLASS_GRAPH            = YES
COLLABORATION_GRAPH    = YES
UML_LIMIT_NUM_FIELDS   = 50
TEMPLATE_RELATIONS     = YES
DOT_GRAPH_MAX_NODES    = 100
MAX_DOT_GRAPH_DEPTH    = 0
DOT_TRANSPARENT        = YES
DOT_IMAGE_FORMAT       = svg

USE_MATHJAX            = YES
HTML_EXTRA_STYLESHEET  = @CMAKE_CURRENT_SOURCE_DIR@/external/doxygen-awesome-css/doxygen-awesome.css
